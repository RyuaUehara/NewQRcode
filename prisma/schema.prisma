generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model staff {
  id        Int      @id @default(autoincrement())
  staffname String?
  active Boolean  @default(true)
}

model visits {
  id        Int      @id @default(autoincrement())
  helperID String?
  helperName String?
  customerName String?
  start_time DateTime?
  end_time DateTime?
  work_status Int? @default(0)
}